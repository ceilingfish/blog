<h1><%= @path %></h1>
<ul id="archives">
  <% if archives.length > 0 %>
    <% for entry in archives %>
      <li>
        <a href="<%= entry.path %>"><%= entry.title %></a>&nbsp;<small>Published <%= entry.date %></small>
      </li>
    <% end %>
  <% else %>
    There are no articles for this date.
  <% end %>
</ul>

